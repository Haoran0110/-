.swiper-tab{  
    width: 100%;  
    border-bottom: 2rpx solid #777777;  
    text-align: center;  
    line-height: 80rpx;
    background-color: #ffffff;
}  
.swiper-tab-list{  font-size: 30rpx;  
    display: inline-block;  
    width: 25%;  
    color: #777777;  
}  
.on{ 
   color: #388E8E;  
    border-bottom: 5rpx solid #388E8E;
}  
.swiper-box{ display: block; 
             height: 100%; 
             width: 100%; 
             overflow: hidden; 
}  
